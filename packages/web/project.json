{
    "name": "web",
    "projectType": "application",
    "root": "packages/web",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "pnpm dlx vite build",
          "cwd": "packages/web"
        },
        "dependsOn": ["^build"]
      },
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite --port 5000",
          "cwd":"packages/web"
        }
      },
      "typecheck":{
        "executor": "nx:run-commands",
        "options":{
          "command": "tsc --noEmit",
          "cwd":"packages/web"
        }
      },
      "preview":{
        "executor": "nx:run-commands",
        "options":{
          "command": "pnpm dlx vite preview --port 5000",
          "cwd":"packages/web"
        }
      }

    }
  }
  